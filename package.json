{
  "name": "storybook-addon-a11y-checklist",
  "version": "1.0.0",
  "description": "A Storybook addon for maintaining WCAG accessibility checklists per story",
  "keywords": [
    "storybook-addons",
    "accessibility",
    "a11y",
    "wcag",
    "checklist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/storybook-a11y-checklist-addon"
  },
  "type": "module",
  "license": "MIT",
  "author": "package-author",
  "bin": {
    "a11y-checklist": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch \"storybook --quiet\"",
    "server": "node scripts/start-server.js",
    "dev": "run-p server storybook",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "boxen": "^7.1.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.1.0",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.8.2",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^4.5.4",
    "zx": "^8.4.1"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "A11Y Checklist",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "html"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
